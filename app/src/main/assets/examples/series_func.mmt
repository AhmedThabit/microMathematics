        <term key="rightTerm" text="2" />
      </term>
    </equation>
    <text_fragment textStyle="subsubsection" numbering="true">
      <term key="text" text="Approximating the sum of two series" />
    </text_fragment>
    <equation>
      <term key="leftTerm" text="S1(t)" />
      <term key="rightTerm" code="plus">
        <term key="leftTerm" code="summation">
          <term key="maxValue" text="t" />
          <term key="index" text="n" />
          <term key="minValue" text="0" />
          <term key="argTerm" code="content:com.mkulesh.micromath.link.f">
            <term key="argTerm" code="mult">
              <term key="leftTerm" text="2" />
              <term key="rightTerm" text="n" />
            </term>
          </term>
        </term>
        <term key="rightTerm" code="summation">
          <term key="maxValue" text="t" />
          <term key="index" text="n" />
          <term key="minValue" text="0" />
          <term key="argTerm" code="content:com.mkulesh.micromath.link.f">
            <term key="argTerm" code="plus">
              <term key="leftTerm" code="mult">
                <term key="leftTerm" text="2" />
                <term key="rightTerm" text="n" />
