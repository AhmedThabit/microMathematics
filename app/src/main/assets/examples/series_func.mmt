          <term key="minValue" text="0" />
          <term key="argTerm" code="log">
            <term key="argTerm" code="content:com.mkulesh.micromath.link.f">
              <term key="argTerm" text="n" />
            </term>
          </term>
        </term>
        <term key="rightTerm" code="log">
          <term key="argTerm" code="product">
            <term key="maxValue" text="t" />
            <term key="index" text="n" />
            <term key="minValue" text="0" />
            <term key="argTerm" code="content:com.mkulesh.micromath.link.f">
              <term key="argTerm" text="n" />
            </term>
          </term>
        </term>
      </term>
    </equation>
