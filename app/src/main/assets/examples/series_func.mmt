            </term>
          </term>
        </term>
        <term key="rightTerm" code="summation">
          <term key="maxValue" code="plus">
            <term key="leftTerm" code="mult">
              <term key="leftTerm" text="z" />
              <term key="rightTerm" text="t" />
            </term>
