        <term key="minValue" text="1" />
        <term key="nextValue" text="2" />
        <term key="maxValue" text="30" />
      </term>
    </equation>
    <equation>
      <term key="leftTerm" text="Δ3(t,z)" />
      <term key="rightTerm" code="minus">
        <term key="leftTerm" code="summation">
          <term key="maxValue" text="t" />
          <term key="index" text="n" />
          <term key="minValue" text="0" />
          <term key="argTerm" code="summation">
            <term key="maxValue" code="minus">
              <term key="leftTerm" text="z" />
              <term key="rightTerm" text="1" />
            </term>
            <term key="index" text="j" />
            <term key="minValue" text="0" />
            <term key="argTerm" code="content:com.mkulesh.micromath.link.f">
              <term key="argTerm" code="plus">
                <term key="leftTerm" code="mult">
                  <term key="leftTerm" text="z" />
                  <term key="rightTerm" text="n" />
                </term>
                <term key="rightTerm" text="j" />
