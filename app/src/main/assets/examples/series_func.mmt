      <term key="xMinValue" text="1" />
      <term key="xFunction" text="t" />
      <term key="xMaxValue" text="30" />
    </plot_function>
    <text_fragment textStyle="subsubsection" numbering="true">
      <term key="text" text="Approximating the double sum of series" />
    </text_fragment>
    <equation>
      <term key="leftTerm" text="z" />
